{
    "tags": {
        "allowUnknownTags": true
    },

    "source": {
        "excludePattern": "(^|\\/|\\\\)_"
    },

    "opts": {
        "destination": "docs",

        "private": true,

        "readme": "README.md",

        "template": "node_modules/ink-docstrap/template"
    },

    "templates": {
        "collapseSymbols": true,

        "cleverLinks": false,

        "monospaceLinks": false,

        "outputSourceFiles": true,

        "theme": "sandstone",

        "inverseNav": true,

        "navType": "inline",

        "linenums": true,

        "dateFormat": "MMMM Do YYYY, h:mm:ss a"
    }
}
